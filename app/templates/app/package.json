{
  "name": "nbgen-appbase",
  "version": "0.0.1",
  "author": "nubevision",
  "description": "Application generated by nbgen 2.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "concurrently -k  -r \"meteor npm run watch\" \"meteor npm run meteor\" || true",
    "android": "concurrently -k  -r \"meteor npm run watch\" \"meteor run android-device --settings=settings.json\" || true",
    "ios": "concurrently -k  -r \"meteor npm run watch\" \"meteor run ios-device --settings=settings.json\" || true",
    "git": "git add . && git commit -am",
    "meteor": "meteor run --settings settings.json",
    "lint": "eslint client server imports",
    "prestart": "meteor npm run lint",
    "pretest": "meteor npm run lint --silent",
    "watch": "meteor npm run gulp",
    "mongo": "meteor mongo",
    "gulp": "gulp --gulpfile .gulpfile.js --cwd .",
    "resgen": "cordova-res && cp resources/android/icon/drawable-xxxhdpi-icon.png public/assets/images/application-logo.png && cp resources/android/icon/drawable-playstore-512-icon.png public/assets/images/application-logo-square.png",
    "nbgenComponent": "yo nbgen:ngcomponent",
    "nbgenCollection": "yo nbgen:collection",
    "deploy-dev": "mkdir -p ../build/dev && npm run gulp build && cd ../deploy/env/dev && mup deploy && cd -",
    "deploy-test": "mkdir -p ../build/test && npm run gulp build && cd ../deploy/env/test && mup deploy && cd -",
    "deploy-prod": "mkdir -p ../build/prod && npm run gulp build && cd ../deploy/env/prod && mup deploy && cd -",
    "build-dev": "mkdir -p ../build/dev && meteor build ../build/dev --server=\"https://nbGenAppBase-dev.nubevtech.com\"",
    "build-test": "mkdir -p ../build/test && meteor build ../build/test --server=\"https://test.nbGenAppBase.com.ph\"",
    "build-prod": "mkdir -p ../build/prod && meteor build ../build/prod --server=\"https://nbGenAppBase.com.ph\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.1.5",
    "@babel/runtime": "^7.1.5",
    "angular": "1.6.6",
    "angular-animate": "1.6.6",
    "angular-aria": "1.6.6",
    "angular-cookies": "1.6.6",
    "angular-expressions": "^0.3.0",
    "angular-i18n": "1.6.6",
    "angular-material": "1.1.17",
    "angular-messages": "1.7.8",
    "angular-meteor": "^1.3.12",
    "angular-resource": "1.6.6",
    "angular-sanitize": "1.6.6",
    "angular-translate": "2.18.1",
    "angular-translate-loader-partial": "2.18.1",
    "angular-translate-storage-cookie": "2.18.1",
    "angular-translate-storage-local": "2.18.1",
    "angular-ui-calendar": "1.0.2",
    "angular-ui-router": "0.4.2",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "busboy-body-parser": "0.3.0",
    "cordova-custom-config": "^5.1.0",
    "easy-otp": "3.0.0",
    "faker": "^4.1.0",
    "html-pdf": "^2.2.0",
    "jsondiffpatch": "^0.3.11",
    "later": "1.2.0",
    "material-steppers": "^2.0.0",
    "mdi": "1.7.22",
    "meteor-node-stubs": "^0.4.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "multer": "1.2.1",
    "ng-file-upload": "12.2.13",
    "oclazyload": "1.0.9",
    "screenfull": "3.3.1",
    "sprintf-js": "1.1.1",
    "underscore": "^1.9.1",
    "underscore.string": "^3.3.5",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "babel-eslint": "^10.0.2",
    "babel-plugin-angularjs-annotate": "^0.10.0",
    "concurrently": "^3.1.0",
    "debug": "^4.1.1",
    "eslint": "^6.1.0",
    "gulp": "^4.0.2",
    "gulp-hjson": "^2.4.3",
    "gulp-inject": "^5.0.4",
    "gulp-string-replace": "^1.1.2",
    "lodash": "^4.17.15"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "angularjs-annotate",
        {
          "explicitOnly": true
        }
      ]
    ]
  }
}
